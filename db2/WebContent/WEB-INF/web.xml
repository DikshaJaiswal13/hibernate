<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>db2</display-name>
  <welcome-file-list>
  <welcome-file>login.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>StudentController</servlet-name>
    <servlet-class>com.pack.controller.StudentController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StudentController</servlet-name>
    <url-pattern>/StudentController</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>viewStudentController</servlet-name>
    <servlet-class>com.pack.controller.viewStudentController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>viewStudentController</servlet-name>
    <url-pattern>/viewStudentController</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>removeStudentController</servlet-name>
    <servlet-class>com.pack.controller.removeStudentController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>removeStudentController</servlet-name>
    <url-pattern>/removeStudentController</url-pattern>
  </servlet-mapping>
  <error-page>
  <exception-type>java.lang.exception</exception-type>
  <location>/error.jsp</location>
  </error-page>
</web-app>